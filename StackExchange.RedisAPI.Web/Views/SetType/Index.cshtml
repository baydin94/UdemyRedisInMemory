@model HashSet<string>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>
<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name"/>
    <button type="submit">KAYDET</button>
</form>

@foreach(var item in Model)
{
    <h2>@item <a asp-controller="SetType" asp-action="DeleteItem" asp-route-name="@item">SİL</a></h2>
}

